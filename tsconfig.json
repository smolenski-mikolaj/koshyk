{
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "lib": [
      "es6"
    ],
    "allowJs": true,
    // "strict": true,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "types": [
      "@types/gapi.auth2",
      "@types/body-parser",
      "@types/cors",
      "@types/express",
      "@types/express-session",
      "@types/node",
      "@types/validator",
      "@types/cookie",
      "@types/js-cookie",
    ],
  },
}
